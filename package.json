{
  "name": "test-website",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "style:eslint": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "style:prettier": "prettier --check 'src/**/*.{ts,tsx}'",
    "style:prettier-write": "prettier --write 'src/**/*.{ts,tsx}'",
    "pre-commit": "npm run style:eslint && npm run style:prettier-write && npm run style:prettier && npm run lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:staged": "test --findRelatedTests",
    "test:coverage": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@loadable/component": "^5.15.3",
    "@tabler/icons-react": "^2.20.0",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint-config-next": "13.4.3",
    "framer-motion": "^10.12.16",
    "lottie-react": "^2.4.0",
    "next": "13.4.3",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "swiper": "^9.3.2",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/loadable__component": "^5.13.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "coveralls": "^3.1.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.1",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "ts-node": "^10.9.1"
  }
}
